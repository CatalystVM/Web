generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["fullTextSearch", "fullTextIndex"]
  binaryTargets   = ["native", "debian-openssl-1.1.x"]
}

datasource db {
  provider = "mysql"
  url      = env("DB_URL")
}

model marketplaceapplication {
  id                  String               @id @db.Char(30)
  name                String
  category_id         String?
  created_at          DateTime             @default(now())
  updated_at          DateTime
  deleted_at          DateTime?
  marketplacecategory marketplacecategory? @relation(fields: [category_id], references: [id], map: "MarketplaceApplication_category_id_fkey")

  @@index([category_id], map: "MarketplaceApplication_category_id_fkey")
}

model marketplacecategory {
  id                     String                   @id @db.Char(30)
  name                   String
  created_at             DateTime                 @default(now())
  updated_at             DateTime
  deleted_at             DateTime?
  marketplaceapplication marketplaceapplication[]
}

model session {
  id         String    @id @db.Char(30)
  auth_token String    @unique(map: "Session_auth_token_key")
  user_id    String
  created_at DateTime  @default(now())
  updated_at DateTime
  deleted_at DateTime?
  user       user      @relation(fields: [user_id], references: [id], map: "Session_user_id_fkey")

  @@index([user_id], map: "Session_user_id_fkey")
}

model user {
  id         String    @id @db.Char(30)
  login_type String?   @default("email")
  email      String?   @unique(map: "User_email_key")
  password   String?
  created_at DateTime  @default(now())
  updated_at DateTime
  deleted_at DateTime?
  session    session[]
}
